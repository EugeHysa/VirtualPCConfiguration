CREATE TABLE RAM (
COD INT(5) AUTO_INCREMENT ,
BRAND varchar(15),
MODEL varchar(100),
RAM_TYPE varchar(15), 
SPEED varchar(15), 
TDP int(11), 
NUMBER_OF_MODULES int(11), 
SIZE_OF_MODULES int(11), 
SIZE int(11), 
PRICE double,
PRIMARY KEY (COD));
ALTER TABLE RAM AUTO_INCREMENT = 3001;


