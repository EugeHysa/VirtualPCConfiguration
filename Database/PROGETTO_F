CREATE SCHEMA `new_schema` ;

create table MOTHERBOARD
(COD int (5) auto_increment,
BRAND varchar (15),
MODEL varchar(50),
SOCKET_CPU varchar (15),
FORMFACTOR varchar(15),
CHIPSET varchar(15),
RAM_SLOTS integer,
RAM_TYPE character (15),
RAM_MAX_GB integer,
TDP int,
PRICE double,
primary key (COD)
);
ALTER TABLE MOTHERBOARD AUTO_INCREMENT=1001;



