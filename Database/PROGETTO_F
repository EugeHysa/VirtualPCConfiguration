create table PCCASE (
COD INT(5) auto_increment,
BRAND varchar(25),
MODEL varchar(25),
CASETYPE varchar(25), 
COLOR varchar(25), 
COMPATIBLE_MOTHERBOARDS varchar(25), 
MAX_HDRIVE_LENGTH double, 
PRICE double,
PRIMARY KEY (COD));
ALTER TABLE PCCASE AUTO_INCREMENT = 7001;

