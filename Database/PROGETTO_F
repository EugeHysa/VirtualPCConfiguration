create table CPU (
COD int(5) AUTO_INCREMENT,
BRAND varchar(25),
MODEL varchar(25),
CPUSOCKET varchar(25), 
FREQUENCY double, 
CORES int(11), 
TDP int(11), 
PRICE double,
PRIMARY KEY(COD));
ALTER TABLE CPU AUTO_INCREMENT=2001;

